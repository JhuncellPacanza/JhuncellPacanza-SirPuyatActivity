class Student:
    def __init__(self, name, age, course):
        self.name = name
        self.age = age
        self.course = course
    
    def introduce(self):
        print(f"Hi, my name is {self.name}, I am {self.age} years old, and I am studying {self.course}.")

student1 = Student("Jhuncell Pacanza", 19, "Information Technology")
student2 = Student("Killua Reyes", 22, "Computer Science")
student3 = Student("Jhunskie Reyes", 19, "Business Administration")

student1.introduce()
student2.introduce()
student3.introduce()
